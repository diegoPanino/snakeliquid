<div class="accordion-item ">
    <h5 id="Head{{ id }}" class="accordion-header">
        <button type="button" class="accordion-button collapsed" data-bs-toggle="collapse" data-bs-target="#Body{{ id }}" aria-expanded="true" aria-controls="Body{{ id }}">
            {{ button}}
        </button>
    </h5>
    <div id="Body{{ id }}" class="accordion-collapse collapse" data-bs-parent="#{{ id }}" aria-labelledby="Head{{ id }}">
        <div class="accordion-body">
            <p class="txt-regular">
                {{ body}}
            </p>
        </div>
    </div>
</div>